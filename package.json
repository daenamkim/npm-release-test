{
  "name": "npm-release",
  "version": "1.0.0",
  "description": "NPM release test",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter spec",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "tslint --project \"./tsconfig.json\"",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha test/*.js - - -R spec",
    "ci": "yarn lint && yarn build && yarn cover",
    "docs": "typedoc --out docs src",
    "gh-pages": "rimraf docs && yarn docs && gh-pages -d docs"
  },
  "keywords": [
    "npm",
    "release"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/istanbul": "^0.4.30",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "gh-pages": "^2.1.1",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.1",
    "tslint": "^5.20.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.4"
  }
}
